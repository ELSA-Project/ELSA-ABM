digraph network { 
	rankdir=TB;
	node [shape=circle, style=filled]
	edge [minlen=2]
	
	/* Nodes */
	%s
	
	/* Edges */
	%s
	
	/* Nodes that have the same rank. */
	%s
	
	%s	// Source Node
	// Sink Node
	
	/* Legend */
	subgraph legend {
		rankdir=TB;
		rank="same";
		
		"legend1" [shape=none, fillcolor="white", fontname="monospace", fontcolor="%s", label=<
			<TABLE BORDER="0">
				<TR><TD>%s</TD></TR>
				<TR><TD>%s</TD></TR>
				<TR><TD>%s</TD></TR>
				<TR><TD>%s</TD></TR>
			</TABLE>>]
	}
}
